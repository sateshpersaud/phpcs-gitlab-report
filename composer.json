{
    "name": "satesh/phpcs-gitlab-report",
    "description": "Output your PHP CodeSniffer report as a GitLab Code Quality report",
    "type": "library",
    "keywords": [
        "phpcs",
        "php-codesniffer",
        "phpcs-report",
        "gitlab",
        "code quality"
    ],
    "homepage": "https://github.com/sateshpersaud/phpcs-gitlab-report",
    "license": "MIT",
    "authors": [
        {
            "name": "Satesh Persaud",
            "email": "sateshpersaud18@gmail.com",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/sateshpersaud/phpcs-gitlab-report/issues",
        "source": "https://github.com/sateshpersaud/phpcs-gitlab-report"
    },
    "require": {
        "ext-json": "*",
        "squizlabs/php_codesniffer": "3.*"
    },
    "require-dev": {
        "phpunit/phpunit": "8.3"
    },
    "autoload": {
        "psr-4": {
            "Satesh\\Phpcs\\": "src"
        }
    }
}
